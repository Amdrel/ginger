{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=900, s-maxage=3600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=3600, s-maxage=3600"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=3600, s-maxage=3600"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400, s-maxage=86400"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=86400, s-maxage=86400"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=86400, s-maxage=86400"
        }
      ]
    }
  ]
}
